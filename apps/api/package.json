{
  "name": "api",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --outdir=dist --minify",
    "start": "node dist/index.js",
    "fmt": "prettier --write . --ignore-path ../../.gitignore",
    "fmt:check": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@fastlane/api": "*",
    "@t3-oss/env-core": "^0.10.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "esbuild": "^0.21.5",
    "ws": "^8.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@fastlane/eslint-config": "*",
    "@fastlane/prettier-config": "*",
    "@types/bun": "^1.1.3",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.14.0",
    "@types/ws": "^8.5.10",
    "eslint": "^9.2.0",
    "prettier": "^3.3.0",
    "tsx": "^4.15.2"
  },
  "prettier": "@fastlane/prettier-config"
}
