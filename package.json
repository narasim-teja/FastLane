{
  "name": "fastlane",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev --turbo",
    "dev:wss": "tsx watch server/ws-server.ts",
    "build": "run-s build:*",
    "build:next": "next build",
    "build:wss": "bun build server/ws-server.ts --target=node --outfile=.next/ws-server.mjs --minify",
    "start": "run-p start:*",
    "start:next": "next start",
    "start:wss": "node .next/ws-server.mjs",
    "preview": "bun run build && bun start",
    "docker": "run-s docker:next docker:wss",
    "docker:next": "node server.js",
    "docker:wss": "node ws-server.mjs",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "fmt": "prettier --write \"**/*.{ts,tsx,mdx,json}\" --cache",
    "fmt:check": "prettier --check \"**/*.{ts,tsx,mdx,json}\" --cache",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun lib/db/migrate.ts",
    "db:drop": "drizzle-kit drop",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit introspect",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "ui": "bunx shadcn-ui@latest",
    "ui-add": "bunx shadcn-ui@latest add",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next",
    "cleani": "rm -rf .next && rm -rf node_modules && bun i",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@oasisprotocol/sapphire-paratime": "^1.3.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@react-email/components": "0.0.19",
    "@react-three/a11y": "^3.0.0",
    "@react-three/drei": "^9.107.0",
    "@react-three/fiber": "^8.16.8",
    "@react-three/rapier": "^1.3.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.45.1",
    "@trpc/client": "^11.0.0-rc.403",
    "@trpc/react-query": "^11.0.0-rc.403",
    "@trpc/server": "^11.0.0-rc.403",
    "@types/ws": "^8.5.10",
    "babel-plugin-react-compiler": "^0.0.0-experimental-592953e-20240517",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "ethers": "^6.13.1",
    "framer-motion": "^11.2.10",
    "geist": "^1.3.0",
    "lenis": "^1.0.42",
    "lucide-react": "^0.376.0",
    "next": "^14.2.4",
    "postgres": "^3.4.4",
    "r3f-perf": "^7.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.0",
    "resend": "^3.3.0",
    "sonner": "^1.5.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "thirdweb": "^5.29.6",
    "three": "^0.165.0",
    "ws": "^8.17.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@next/bundle-analyzer": "^14.2.4",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/cors": "^2.8.17",
    "@types/node": "20.12.7",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@types/three": "^0.165.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "autoprefixer": "10.4.19",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.22.7",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "husky": "^9.0.11",
    "jiti": "^1.21.0",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sharp": "^0.33.4",
    "tailwindcss": "3.4.3",
    "tsx": "^4.15.7",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.13.1"
  },
  "lint-staged": {
    "*.{ts,tsx,md,mdx}": [
      "prettier --write \"**/*.{ts,tsx,mdx}\" --cache"
    ],
    "*.{ts,tsx,mdx}": [
      "eslint --fix ."
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=20.12.2"
  }
}
