{
  "name": "fastlane",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo | pino-pretty",
    "build": "next build",
    "start": "next start | pino-pretty",
    "preview": "next build && next start | pino-pretty",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "fmt": "prettier --write \"**/*.{ts,tsx,mdx,json}\" --cache",
    "fmt:check": "prettier --check \"**/*.{ts,tsx,mdx,json}\" --cache",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun lib/db/migrate.ts",
    "db:drop": "drizzle-kit drop",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit introspect",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "ui": "bunx shadcn-ui@latest",
    "ui-add": "bunx shadcn-ui@latest add",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next",
    "cleani": "rm -rf .next && rm -rf node_modules && bun i",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@oasisprotocol/sapphire-paratime": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@react-email/components": "0.0.19",
    "@react-three/a11y": "^3.0.0",
    "@react-three/drei": "^9.108.4",
    "@react-three/fiber": "^8.16.8",
    "@react-three/rapier": "=1.3.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.51.11",
    "@trpc/client": "^11.0.0-rc.466",
    "@trpc/react-query": "^11.0.0-rc.466",
    "@trpc/server": "^11.0.0-rc.466",
    "@types/ws": "^8.5.11",
    "babel-plugin-react-compiler": "^0.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.31.4",
    "ethers": "^6.13.1",
    "framer-motion": "^11.3.8",
    "geist": "^1.3.1",
    "lenis": "^1.1.6",
    "lucide-react": "^0.376.0",
    "next": "^14.2.5",
    "pino": "^9.3.1",
    "postgres": "^3.4.4",
    "r3f-perf": "^7.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "resend": "^3.4.0",
    "sonner": "^1.5.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "thirdweb": "^5.37.0",
    "three": "^0.165.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@next/bundle-analyzer": "^14.2.5",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/cors": "^2.8.17",
    "@types/node": "20.12.7",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@types/three": "^0.165.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "autoprefixer": "10.4.19",
    "drizzle-kit": "^0.22.8",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "husky": "^9.1.1",
    "jiti": "^1.21.6",
    "lint-staged": "^15.2.7",
    "pino-pretty": "^11.2.1",
    "postcss": "8.4.38",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sharp": "^0.33.4",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.16.1"
  },
  "lint-staged": {
    "*.{ts,tsx,md,mdx}": [
      "prettier --write \"**/*.{ts,tsx,mdx}\" --cache"
    ],
    "*.{ts,tsx,mdx}": [
      "eslint --fix ."
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=20.12.2"
  }
}
