{
  "name": "@fastlane/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "fmt": "prettier --write . --ignore-path ../../.gitignore",
    "fmt:check": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "type-check": "tsc --noEmit --emitDeclarationOnly false",
    "clean": "rm -rf .turbo node_modules"
  },
  "dependencies": {
    "@oasisprotocol/sapphire-paratime": "^1.3.2",
    "@trpc/server": "^11.0.0-rc.364",
    "ethers": "^6.13.0",
    "superjson": "2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@fastlane/eslint-config": "*",
    "@fastlane/prettier-config": "*",
    "@fastlane/tsconfig": "*",
    "eslint": "^9.2.0",
    "prettier": "^3.3.0",
    "typescript": "^5.4.5"
  },
  "prettier": "@fastlane/prettier-config"
}
