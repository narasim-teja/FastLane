{
  "name": "@fastlane/tailwind-config",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "fmt": "prettier --write \"**/*.{ts,tsx,mdx,json}\" --cache --experimental-ternaries",
    "fmt:check": "prettier --check \"**/*.{ts,tsx,mdx,json}\" --cache --experimental-ternaries",
    "lint": "eslint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@fastlane/eslint-config": "*",
    "@fastlane/prettier-config": "*",
    "@fastlane/typescript-config": "*",
    "eslint": "8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "prettier": "@fastlane/prettier-config",
  "publishConfig": {
    "access": "public"
  }
}
