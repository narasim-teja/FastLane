{
  "name": "@fastlane/prettier-config",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "fmt": "prettier --write \"**/*.{ts,tsx,mdx,json}\" --cache --experimental-ternaries",
    "fmt:check": "prettier --check \"**/*.{ts,tsx,mdx,json}\" --cache --experimental-ternaries",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.1"
  },
  "devDependencies": {
    "@fastlane/typescript-config": "*",
    "typescript": "^5.4.5"
  },
  "prettier": "@fastlane/prettier-config",
  "publishConfig": {
    "access": "public"
  }
}
